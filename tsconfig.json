{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es6",
    "lib": ["dom", "es2015", "es2017"],
    "baseUrl": "src/",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "removeComments": true,
    "noResolve": false
  }
}
